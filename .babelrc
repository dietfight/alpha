{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
        [
          "react-transform",
          {
            "transforms": [{
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }]
          },
        ],
        'transform-runtime', 'transform-decorators-legacy', 'add-module-exports'
  ]
}
